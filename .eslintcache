] = useState('');\r\n    const [lastName, setLastName] = useState('');\r\n    const [email, setEmail] = useState('');\r\n    const [password, setPassword] = useState('');\r\n    const [username,setUsername] = useState('');\r\n    const sendData = ()=>{\r\n        // checking for valid email using regex expression\r\n        if(!/^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\\.[a-zA-Z0-9-]+)*$/.test(email)){\r\n            M.toast({html:\"Invalid email...\",classes:\"#e53935 red darken-1\"});\r\n            return\r\n        }\r\n        fetch('/user/signup',{\r\n            method:'post',\r\n            headers:{\r\n                'Content-Type':'application/json'\r\n            },\r\n            body:JSON.stringify({\r\n                firstName,\r\n                lastName,\r\n                email,\r\n                username,\r\n                password\r\n            })\r\n        }).then(res=>res.json())\r\n        .then(data=>{\r\n            console.log(data);\r\n            if(data.error){\r\n                  M.toast({html:data.message,classes:\"#e53935 red darken-1\"});\r\n               //  history.push('/singup');\r\n            }\r\n            else if(data.status == true){\r\n                M.toast({html:data.message,classes:\"#4caf50 green\"});\r\n                history.push('/login');\r\n            }\r\n            else{\r\n                M.toast({html:data.message,classes:\"#4caf50 green\"});\r\n                history.push('/signup');\r\n            }\r\n        })\r\n    }\r\n    return(\r\n        <div className = \"mycard\">\r\n        <div className = \"card\">\r\n        <h2>Instagram</h2>\r\n        <input type=\"text\" placeholder=\"firstname\" value={firstName} onChange={(e)=>setFirstName(e.target.value)}></input>\r\n        <input type=\"text\" placeholder=\"lastname\" value={lastName} onChange={(e)=>setLastName(e.target.value)}></input>\r\n        <input type=\"text\" placeholder=\"email\" value={email} onChange={(e)=>setEmail(